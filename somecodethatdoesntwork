def get_inv(conn,c,user,item,itemnum)
itemnum = "0"
    num = 0
    ha = c.fetchone()
    while ha != None:
        item = "item"
        num = num + 1
        num = str(num)
        itemnum = item + num
        num = int(num)
        try:
            c.execute("SELECT ? FROM inv WHERE user = ?",(itemnum,user,)) # i really want to get this one working but i wont lol    what it doo?  selects the nearest place to the start with a null value
            
        except:
            break
        c.execute("UPDATE inv SET ? = ? WHERE user = ? ",(itemnum,item,user,))  # idk how to get it that it puts it in a unspecified place :/
        conn.commit()





